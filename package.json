{
  "name": "ntedcraper",
  "version": "0.0.1",
  "description": "NodeJS Ted Scraper",
  "main": "index.js",
  "repository": "https://github.com/ArthurAssuncao/NTedScraper.git",
  "author": "Arthur Assuncao",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "^1.2.3",
    "cheerio": "^1.0.0-rc.12"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "babel src --out-dir dist --no-copy-ignored",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "yarn lint",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "release": "standard-version",
    "packages:upgrade:check": "npx npm-check-updates",
    "packages:upgrade:do": "ncu -u && yarn install"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jest": "^29.2.6",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "crypto": "^1.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "npm-check-updates": "^16.6.2",
    "pinst": "^3.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
}
